{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/data/*", "/*.{css,js,json,m4a}"]
  },
  "mimeTypes": {
    ".m4a": "audio/mp4"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "routes": [
    {
      "route": "/data/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
